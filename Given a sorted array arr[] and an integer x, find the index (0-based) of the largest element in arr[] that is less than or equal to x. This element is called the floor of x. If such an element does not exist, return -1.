class Solution:
    def findFloor(self, arr, x):
        low = 0
        high = len(arr) - 1
        ans = -1   # stores index
        
        while low <= high:
            mid = (low + high) // 2
            
            if arr[mid] <= x:
                ans = mid        # store index
                low = mid + 1
            else:
                high = mid - 1
        
        return ans
