# Stack class template
class myStack:

    def __init__(self):
        self.top = None
        self.count = 0

    def isEmpty(self):
        return self.top is None

    def push(self, x):
        new_node = Node(x)
        new_node.next = self.top
        self.top = new_node
        self.count += 1

    def pop(self):
        if self.isEmpty():
            return -1
        
        popped = self.top.data
        self.top = self.top.next
        self.count -= 1
        return popped

    def peek(self):
        if self.isEmpty():
            return -1
        return self.top.data

    def size(self):
        return self.count
